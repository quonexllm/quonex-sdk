{
  "name": "@quonexai/sdk",
  "version": "1.0.0",
  "description": "Solana TypeScript SDK for QuonexAI LLM instructions",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src test --max-warnings=0",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "solana",
    "sdk",
    "typescript",
    "quonexai",
    "llm"
  ],
  "author": "QuonexAI",
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.95.0",
    "bn.js": "^5.2.1",
    "borsh": "^0.7.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  }
}
